{
  "manifest_version": 3,
  "name": "Thesis",
  "description": "A quick way to browse top posts from DEV Community.",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting",
    "idle",
    "clipboardRead",
    "alarms"
  ],
  "host_permissions": ["*://docs.google.com/forms/*"]
}
